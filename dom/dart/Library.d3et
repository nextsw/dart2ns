class Library {
    String fullPath;
    String packagePath;
    String id;
    Library parent;
    List<Import> imports = [];
    List<Export> exports = [];
    List<TopDecl> objects = [];
    List<Part> parts = [];

    Library({this.parent, this.fullPath, this.packagePath});
    

    TopDecl get(String name) {
        return objects.firstWhere(o => o.name == name);
    }

}