server class CaseExpression {
    List<Expression> tests = [];
    Expression result;
    Range range;

    DataType expType;

    void resolve(ResolveContext context) {
        tests.forEach((c) {
            c.resolve(context);
        });
        result.resolve(context);
    }
    void collectUsedTypes(List<DataType> types) {
        tests.forEach((c) {
            c.collectUsedTypes(types);
        });
        result.collectUsedTypes(types);
    }    
}